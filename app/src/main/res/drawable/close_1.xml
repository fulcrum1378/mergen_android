<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="80dp"
        android:right="-9dp"
        android:left="10dp"
        android:top="5dp">
        <rotate
            android:fromDegrees="45"
            android:pivotX="0%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <corners android:radius="8dp" />
                <solid android:color="@color/CS" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="80dp"
        android:left="-9dp"
        android:right="10dp"
        android:top="5dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="100%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <corners android:radius="8dp" />
                <solid android:color="@color/CS" />
            </shape>
        </rotate>
    </item>
</layer-list>